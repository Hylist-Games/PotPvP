        <dependency>
            <groupId>net.frozenorb</groupId>
            <artifactId>Hydrogen</artifactId>
            <version>1.0</version>
        </dependency>

        <!-- Misc. Jars -->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>1.16.10</version>
        </dependency>
        <dependency>
            <groupId>com.sk89q</groupId>
            <artifactId>worldedit</artifactId>
