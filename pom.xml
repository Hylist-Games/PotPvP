    <parent>
        <groupId>net.frozenorb</groupId>
        <artifactId>minehq-parent</artifactId>
        <version>1.0</version>
    </parent>

    <properties>
        <minehq.mavenLevel>minehq-high</minehq.mavenLevel>
    </properties>

    <repositories>
        <repository>
            <id>minehq-repo</id>
            <url>http://maven.minehq.com:8081/artifactory/minehq-all/</url>
